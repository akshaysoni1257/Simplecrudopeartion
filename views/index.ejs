<table border="1">  
           <tr>
            <td>Name</td>
            <td>Email</td>
            <td>Phone</td>
            <td>Action</td>
            
           </tr>

           <% for(let i=0; i<record.length; i++) { %>

            <tr>
                <td><%= record[i].name %></td>
                <td><%= record[i].email %></td>
                <td><%= record[i].phone %></td>
                <td>
                    <a href="/deletedata/?id=<%= record[i].phone %>">Delete</a>||
                    
                    <a href="/editdata/<%= record[i].phone %>">Edit</a>
                </td>
            </tr>
        <% } %>
</table>

<form action="/insertdata" method="post">
    <table border="1">
        <tr>
            <td>Name:</td>
            <td><input type="text" name="name"></td>
        </tr>
        <tr>
            <td>Email:</td>
            <td><input type="text" name="email"></td>
        </tr>
        <tr>
            <td>Phone:</td>
            <td><input type="text" name="phone"></td>
        </tr>

        <tr>
            <td></td>
            <td><input type="submit"></td>
        </tr>
    </table>
</form>